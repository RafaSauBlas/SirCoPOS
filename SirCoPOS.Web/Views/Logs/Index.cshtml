@model IEnumerable<SirCoPOS.DataAccess.SirCoLogs.Log>
@{
    ViewBag.Title = "Logs";
}

<h2>Logs</h2>

<table class="table table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>Date</th>
            <th>Application</th>
            <th>Module</th>
            <th>MachineName</th>
            <th>Level</th>
            <th>Type</th>
            <th>Message</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        { 
        <tr>
            <td><a href="@Url.Action("Detail", new { id = item.Id })">@item.Id</a></td>
            <td>@item.Date</td>
            <td>@item.Application</td>
            <td>@item.Module</td>
            <td>@item.MachineName</td>
            <td>@item.Level</td>
            <td>@item.Type</td>
            <td>@item.Message</td>
        </tr>
        }
    </tbody>
</table>