<Window x:Class="SirCoPOS.Win.Windows.ViewerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SirCoPOS.Win.Windows"
        xmlns:rpt="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"
        mc:Ignorable="d"
        Title="Viewer" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <WindowsFormsHost Margin="10">
            <rpt:ReportViewer x:Name="reportViewer"></rpt:ReportViewer>
        </WindowsFormsHost>
    </Grid>
</Window>
