<Window x:Class="SirCoPOS.Win.Windows.DebugWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SirCoPOS.Win.Windows"
        mc:Ignorable="d"
        Title="DebugWindow" Height="350" Width="600">
    <Grid>
        <TextBox AcceptsReturn="True" AcceptsTab="True" x:Name="textBox" Margin="10,10,10,43" TextWrapping="Wrap" />
        <Button Content="Debug" Margin="0,0,10,10" HorizontalAlignment="Right" Width="75" Height="28" VerticalAlignment="Bottom" Click="Button_Click"/>
        <ComboBox x:Name="comboBoxCall" Margin="0,0,90,10" HorizontalAlignment="Right" Width="120" Height="22" VerticalAlignment="Bottom">
            <ComboBoxItem Content="Sale" />
            <ComboBoxItem Content="CheckPromociones" />
            <ComboBoxItem Content="Return" />
        </ComboBox>

    </Grid>
</Window>
